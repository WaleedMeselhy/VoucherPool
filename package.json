{
  "name": "voucher-pool",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "migrate:prod": "prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:create-sql": "prisma migrate dev --create-only --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.4.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^7.1.14",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/throttler": "^5.0.1",
    "@prisma/client": "^5.6.0-dev.40",
    "@types/ip": "^1.1.0",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/uuid": "^8.3.4",
    "axios-retry": "^3.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.3",
    "eslint-config-google": "^0.14.0",
    "ioredis": "^5.3.2",
    "ip": "^1.1.5",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "moment": "^2.29.1",
    "nanoid": "3",
    "nestjs-cls": "^2.0.0",
    "nestjs-i18n": "^8.2.2",
    "nestjs-throttler-storage-redis": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "set-cookie-parser": "^2.4.8",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "prisma": "^5.6.0-dev.40",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
